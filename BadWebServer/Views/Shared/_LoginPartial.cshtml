@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@if (this.Context.Request.Cookies.ContainsKey("email"))
{
<li class="nav-item">
    <a class="nav-link" asp-action="profile" asp-controller="home">Profile</a>
</li>

    <li class="nav-item">
        <a class="nav-link" asp-action="SignOut" asp-controller="home">Sign Out from @this.Context.Request.Cookies["email"]</a>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link" asp-action="register" asp-controller="home">Register</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" asp-action="signin" asp-controller="home">Sign In</a>
    </li>
}